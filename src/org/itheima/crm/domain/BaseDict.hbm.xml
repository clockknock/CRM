<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.itheima.crm.domain.BaseDict" table="base_dict" schema="crm">
        <id name="dictId">
            <column name="dict_id" sql-type="bigint(20)"/>
            <generator class="native"/>
        </id>
        <property name="dictTypeCode">
            <column name="dict_type_code" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="dictTypeName">
            <column name="dict_type_name" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="dictItemName">
            <column name="dict_item_name" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="dictItemCode">
            <column name="dict_item_code" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="dictSort">
            <column name="dict_sort" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="dictState">
            <column name="dict_state" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="dictComment">
            <column name="dict_comment" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>